{
  "name": "sveltekit-typescript-postcss-tailwind2",
  "version": "0.0.4",
  "scripts": {
    "dev": "svelte-kit dev --host 0.0.0.0 --port 3333",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write ./src/**/*.{svelte,ts,js} ./*.{ts,js}",
    "lint": "eslint ./src/**/*.{svelte,ts,js} --ignore-path .gitignore .",
    "lint:fix": "eslint --fix ./src/**/*.{svelte,ts,js} --ignore-path .gitignore ."
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "autoprefixer": "^10.3.6",
    "cssnano": "^5.0.8",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "postcss": "^8.3.8",
    "postcss-load-config": "^3.1.0",
    "postcss-nested": "^5.0.6",
    "prettier": "~2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.43.1",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.7",
    "tailwindcss": "^2.2.16",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "type": "module"
}
